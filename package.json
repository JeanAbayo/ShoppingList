        "react-meta-tags": "^0.3.0",
        "react-modal": "^3.1.13",
        "react-redux": "^5.0.6",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.9",
        "react-scripts": "1.0.17",
        "react-spinkit": "^3.0.0",
        "react-toastify": "^3.2.2",
